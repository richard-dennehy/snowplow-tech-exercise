POST    /schema/:schemaId           controllers.JsonSchemaController.uploadSchema(schemaId)
GET     /schema/:schemaId           controllers.JsonSchemaController.getSchema(schemaId)

POST    /validate/:schemaId         controllers.JsonSchemaController.validate(schemaId)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

      